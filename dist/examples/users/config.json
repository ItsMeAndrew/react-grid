{
    "title": "Github Users",
	"rows_per_page": 10,
    "paging_options": [5, 10, 20, 50, 100, 250],
    "show_paging": true,
    "columns": [
        {
            "id": "avatar_url",
            "index": 0,
            "name": null,
            "sortable": false,
			"ascending": false,
            "style": {
                "width": 30
            },
			"type": {
				"name": "image",
                "src": "avatar_url",
                "props": {
                    "style": {
                        "width": 20,
                        "backgroundColor": "#fff",
                        "padding": 3,
                        "borderRadius": "50%"
                    }
                }
			}
        },
		{
            "id": "login",
            "index": 1,
            "name": "Username",
            "sortable": true,
			"ascending": false,
			"type": {
				"name": "link",
                "display": "login",
                "href": "html_url",
                "props": {
                    "rel": "View github repo",
                    "target": "_blank"
                }
			}
        }
    ]
}
